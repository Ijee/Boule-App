package Turnierplan.uebersicht;

import Main.*;
import javafx.event.Event;
import javafx.event.EventHandler;
import javafx.scene.layout.*;
import javafx.scene.control.*;


/**
 * Die UI-Klasse der uebersichtVision.
 * 
 * @author Thorsten Schulz, Alexander Röber
 */
public class UebersichtVisionUI
{
    private Master master;
    private EventHandler<Event> handler;
    
    private Pane content;
    
    public UebersichtVisionUI(Master m, EventHandler<Event> handler)
    {
        master = m;
        this.handler = handler;
    }
    
    /**
     * Gibt die contentpane zurück.
     * 
     * @return die contentpane
     */
    public Pane getPane()
    {
        return content;
    }

}

